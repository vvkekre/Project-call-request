{
  "name": "@dasha.ai/sdk",
  "version": "0.6.7",
  "description": "Dasha.AI SDK",
  "license": "Apache-2.0",
  "author": {
    "name": "Dasha AI",
    "url": "https://dasha.ai/en-us"
  },
  "bugs": {
    "url": "https://github.com/dasha-ai/dasha/issues"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./account": "./dist/account.js",
    "./audio": "./dist/audio.js",
    "./audio-client": "./dist/audio-client.js",
    "./auth": "./dist/auth.js",
    "./applications": "./dist/applications.js",    
    "./chat": "./dist/chat.js",
    "./diagnostics": "./dist/diagnostics.js",
    "./errors": "./dist/errors.js",
    "./groupd": "./dist/groups.js",
    "./sip": "./dist/sip.js",
    "./stt": "./dist/stt.js",
    "./tts": "./dist/tts.js"
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index"],
      "./account": ["./dist/account"],
      "./audio": ["./dist/audio"],
      "./audio-client": ["./dist/audio-client"],
      "./auth": ["./dist/auth"],
      "./applications": ["./dist/applications"],      
      "./chat": ["./dist/chat"],
      "./diagnostics": ["./dist/diagnostics"],
      "./errors": ["./dist/errors"],
      "./groupd": ["./dist/groups"],
      "./sip": ["./dist/sip"],
      "./stt": ["./dist/stt"],
      "./tts": ["./dist/tts"]
    }
  },
  "engines": {
    "node": ">=13.1.0"
  },
  "scripts": {
    "ci": "npm run lint && npm run build && npm run build:docs && npm pack",
    "download:jre": "node scripts/download-jre.js",
    "generate:json": "node scripts/generate-json.js",
    "generate:openapi": "node scripts/generate-openapi.js",
    "generate:proto": "node scripts/generate-proto.js",
    "prepare": "npm run download:jre && npm run generate:json && npm run generate:proto && npm run generate:openapi",
    "lint": "eslint .",
    "build": "tsc",
    "build:docs": "typedoc"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.23",
    "@types/archiver": "^5.1.0",
    "@types/co-body": "^5.1.0",
    "@types/google-protobuf": "^3.7.4",
    "@types/proxy-from-env": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "grpc-tools": "^1.10.0",
    "grpc_tools_node_protoc_ts": "^5.1.1",
    "json-schema-to-typescript": "^10.1.3",
    "njre": "^0.2.0",
    "pascal-case": "^3.1.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.2.12",
    "ajv": "^7.1.1",
    "archiver": "^5.2.0",
    "axios": "^0.21.4",
    "co-body": "^6.1.0",
    "env-paths": "^2.2.0",
    "eventemitter3": "^4.0.7",
    "google-protobuf": "^3.14.0",
    "openid-client": "^4.4.2",
    "proxy-agent": "^5.0.0",
    "proxy-from-env": "^1.1.0",
    "querystring": "^0.2.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "optionalDependencies": {
    "winston-transport-sentry-node": "^1.0.2"
  }
}
